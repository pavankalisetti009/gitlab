{
  "name": "duo-next",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "prettier": "@frontend-islands/prettier-config",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:watch": "vite build --watch",
    "build:prod": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "shadcn": "shadcn-vue",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.542.0",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "vite": "^7.1.2",
    "vitest": "^2.1.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@frontend-islands/eslint-config": "0.1.0",
    "@frontend-islands/prettier-config": "0.1.0",
    "@frontend-islands/tsconfig": "0.1.0",
    "@frontend-islands/vite-config": "0.1.0",
    "@frontend-islands/vitest-config": "0.1.0",
    "@types/node": "^22.12.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^25.0.0",
    "prettier": "^3.6.2",
    "shadcn-vue": "^2.2.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vue-tsc": "^3.0.5"
  }
}
