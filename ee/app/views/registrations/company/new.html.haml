- page_title s_('Trial|Start your free Ultimate and GitLab Duo Enterprise trial')

- content_for :page_specific_javascripts do
  = render "layouts/google_tag_manager_head"
  = render "layouts/one_trust"
  = render "layouts/bizible"
= render "layouts/google_tag_manager_body"

.row.gl-max-w-4xl.gl-mx-auto.gl-mt-6.gl-justify-center
  .col-md-8.gl-px-9
    %h2.gl-pb-5.gl-my-0.gl-text-center
      = _("Tell us about your company")
    #js-registrations-company-form{ data: create_company_form_data(onboarding_status_presenter) }

  - if onboarding_status_presenter.show_company_form_side_column?
    = render 'shared/trial_reassurances'
